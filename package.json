{
  "name": "humanitas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "git-hook": "npm run lint -s && npm run test -s",
    "lint": "standard --no-verify | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix"
  },
  "dependencies": {
    "apisauce": "^0.14.0",
    "blueimp-md5": "^2.8.0",
    "epubjs": "^0.3.57",
    "epubjs-rn": "^0.0.113",
    "format-json": "^1.0.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "querystringify": "0.0.4",
    "ramda": "^0.24.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.1",
    "react-native-animatable": "^1.2.3",
    "react-native-collapsible": "^0.9.0",
    "react-native-drawer": "^2.3.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.5.1",
    "react-native-google-analytics-bridge": "^5.4.1",
    "react-native-htmlview": "^0.12.0",
    "react-native-i18n": "1.0.0",
    "react-native-img-cache": "^1.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-material-dialog": "^0.5.4",
    "react-native-material-ui": "^1.12.0",
    "react-native-music-control": "^0.4.7",
    "react-native-orientation": "^3.0.0",
    "react-native-progress": "^3.4.0",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-share": "github:valinaga/react-native-share",
    "react-native-simple-auth": "^2.1.0",
    "react-native-splash-screen": "^3.0.0",
    "react-native-static-server": "^0.2.0",
    "react-native-textinput-effects": "^0.4.0",
    "react-native-vector-icons": "^4.3.0",
    "react-native-video": "^2.0.0",
    "react-native-wkwebview-reborn-fchasen": "1.8.1",
    "react-native-zip-archive": "github:mockingbot/react-native-zip-archive",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "2.0.0",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^1.0.0",
    "ignite-dev-screens": "^2.0.1",
    "ignite-i18n": "^1.0.0",
    "ignite-ir-boilerplate": "^2.0.2",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.0.0",
    "jest": "20.0.4",
    "jest-react-native": "18.0.0",
    "mockery": "^2.0.0",
    "patch-package": "^3.4.5",
    "react-addons-test-utils": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "reactotron-apisauce": "^1.12.2",
    "reactotron-react-native": "^1.12.0",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "snazzy": "^7.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
